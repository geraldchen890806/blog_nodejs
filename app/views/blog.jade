extends layout

block content
  .blog-detail
    div.blog
      header.article-header
        h1
          a(class="article-title", href="javascript:void(0)")= blog.title
      .content.article-entry
        != blog.content
      footer.article-footer
        ul.article-tag-list
          li.article-tag-list-item
            a.article-tag-list-link= blog.tag
script
  $(".content").each()